<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginStart="@dimen/_10sdp">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_time_schedules"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Text.Small.Bold"
        android:text="@string/time_schedules"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_description_schedules"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/Text.Small"
        android:text="@string/paket_lainnya"
        app:layout_constraintTop_toBottomOf="@+id/tv_time_schedules"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>